lass Solution:
    def romanToInt(self, s: str) -> int:
        d={'I':1,'IV':4,'V':5,'IX':9,'X':10,'XL':40,'L':50,'XC':90,'C':100,'CD':400,'D':500,'CM':900,'M':1000}
        n=0
        while len(s)>0:
            if s[0:2] in d.keys():
                n=n+d[s[0:2]]
                s=s[2:]
            elif s[0] in d.keys():
                n=n+d[s[0]]
                s=s[1:]
        return n
