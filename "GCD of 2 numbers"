while True:
    n=int(input("enter the first number:"))
    k=int(input("enter the second number:"))
    a=[]
    b=[]
    l=[]
    for i in range(1,n):
        if n%i==0:
            a.append(i)
    for j in range(1,k):
        if k%j==0:
            b.append(j)
    if len(a)>len(b) or len(a)<len(b) or len(a)==len(b):
        for i in range(len(a)):
            for j in range(len(b)):
                if a[i]==b[j]:
                    l.append(a[i])
    print(l[-1])
