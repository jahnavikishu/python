while True:
    n=int(input("enter the first number:"))
    k=int(input("enter the second number:"))
    a=[]
    b=[]
    l=[]
    
    for i in range(1,n+1):
        if n%i==0:
            a.append(i)
    for j in range(1,k+1):
        if k%j==0:
            b.append(j)
    for i in a:
        for i in b:
            l.append(i)
    print(l[-1])
